# restaurant_expense_tracker/.env
# PostgreSQL Database Credentials for MLflow Backend Store
POSTGRES_DB=mlflow_db
POSTGRES_USER=mlflow_user
POSTGRES_PASSWORD=mlflow_password_secure

# MLflow Tracking URI (used by ml_experiment.py)
# This will be passed as an environment variable to the containers
MLFLOW_TRACKING_URI=http://mlflow-server:5000 # Service name 'mlflow-server'
MLFLOW_ARTIFACT_URI=s3://mlflow-artifacts # Example for S3, or file:/mlflow_artifacts for local volume

# For Streamlit app to connect to MLflow (if needed directly)
# MLFLOW_TRACKING_URI_STREAMLIT=http://mlflow-server:5000